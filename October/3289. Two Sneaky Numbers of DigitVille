class Solution {
    public int[] getSneakyNumbers(int[] nums) {
        int n = nums.length;
        int temp = 0;
        int[] ans = new int[2];
        for(int i=0; i < n; i++) {
            for(int j = i+1; j < n; j++) {
                if(i+1 == n) return ans;
                if(nums[i] == nums[j]) ans[temp++] = nums[i];
            }
        }
        return ans;
    }
}
